
<footer  class="footer" role="contentinfo">
    <div class="footer_top">
        <div class="container">
            <div class="newsLetter_wrap">
            <div class="row justify-content-between">
                <div class="col-md-7">
                <div class="footer_widget">
                    <h3 class="footer_title">
                        {{ 'Stay Updated'|t }}
                    </h3>
                    {{ drupal_entity('block_content', 2) }}
                </div>
                </div>
                <div class="col-md-12 col-lg-5">
                <div class="footer_widget">
                    <h3 class="footer_title">
                        {{ 'Stay Updated'|t }}
                    </h3>
                    <div class="socail_links">
                    <ul>
                        <li>
                        <a target="_blank" href="{{ facebook_url }}">
                            <i class="ti-facebook"></i>
                        </a>
                        </li>
                        <li>
                        <a href="{{ whatsapp_url }}">
                            <i class="fa fa-whatsapp"></i>
                        </a>
                        </li>
                        <li>
                        <a target="_blank" href="{{ instagram_url }}">
                            <i class="fa fa-instagram"></i>
                        </a>
                        </li>
                        <li>
                        <a target="_blank" href="{{ linkedin_url }}">
                            <i class="fa fa-linkedin"></i>
                        </a>
                        </li>
                    </ul>
                    </div>
                </div>
                </div>
            </div>
            </div>
            <div class="row">
                {{ drupal_region('footer_menus')|without('#theme_wrappers') }}
            </div>
        </div>
    </div>
    <div class="copy-right_text">
        <div class="container">
            <div class="footer_border"></div>
            <div class="row">
            <div class="col-xl-12">
                <p class="copy_right text-center">
                    {{ drupal_region('footer_copyright') }}
                </p>
            </div>
            </div>
        </div>
    </div>
 {{ page.footer_fifth }}
</footer>
